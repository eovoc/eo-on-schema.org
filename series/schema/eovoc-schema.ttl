    schema:rangeIncludes schema:Text ;
    rdfs:comment "Acquisition / receiving station code. Possible values are mission specific and should be retrieved using codespace." .

schema:acquisitionStatus a rdf:Property ;
    rdfs:label "acquisitionStatus" ;
    dcterms:source <http://docs.opengeospatial.org/is/17-003r2/17-003r2.html> ;
    schema:domainIncludes schema:EarthObservation ;
    schema:isPartOf <http://eo.schema.org> ;
    schema:rangeIncludes schema:AcquisitionStatusType ;
    rdfs:comment """Refers to product status.

Fixed Values :
 - ARCHIVED
 - ACQUIRED
 - CANCELLED
 - FAILED
 - PLANNED
 - POTENTIAL
 - REJECTED
 - QUALITYDEGRADED.""";
    owl:equivalentProperty <http://www.opengis.net/ont/eo-geojson/1.0/status> .

