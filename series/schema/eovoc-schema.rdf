    <schema:rangeIncludes rdf:resource="http://schema.org/OrbitDirectionType"/>
    <schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
    <rdfs:label>orbitDirection</rdfs:label>
    <schema:isPartOf rdf:resource="http://eo.schema.org"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/acquisitionParameters">
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionInformation"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:label>acquisitionParameters</rdfs:label>
    <rdfs:comment>The acquisition parameters (i.e. pointing angles, etc.).</rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
  </rdf:Property>
  <schema:PolarisationChannelsType rdf:about="http://schema.org/HH_VV">
    <rdfs:label>HH_VV</rdfs:label>
    <rdfs:comment>Polarisation channel constant.</rdfs:comment>
  </schema:PolarisationChannelsType>
  <schema:AcquisitionStatusType rdf:about="http://schema.org/POTENTIAL">
    <rdfs:comment>Acquisition status constant.</rdfs:comment>
    <rdfs:label>POTENTIAL</rdfs:label>
  </schema:AcquisitionStatusType>
  <rdf:Property rdf:about="http://schema.org/antennaLookDirection">
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:comment>Look direction of antenna taken from codelist
Values:
- LEFT
- RIGHT.</rdfs:comment>
    <rdfs:label>antennaLookDirection</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/AntennaLookDirectionType"/>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/incidenceAngleVariation">
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Number"/>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionAngles"/>
    <rdfs:label>incidenceAngleVariation</rdfs:label>
    <rdfs:comment>Incidence angle variation in degrees (i.e. uom='deg').</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/holdingArchive">
    <schema:domainIncludes rdf:resource="http://schema.org/ProductInformation"/>
    <owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/archivingCenter"/>
  </rdf:Property>
  <schema:SensorType rdf:about="http://schema.org/OPTICAL">
    <rdfs:comment>Sensor type constant.</rdfs:comment>
    <rdfs:label>OPTICAL</rdfs:label>
  </schema:SensorType>
  <rdf:Property rdf:about="http://schema.org/processingMethod">
    <rdfs:label>processingMethod</rdfs:label>
    <rdfs:comment>Method used to compute datalayer. (e.g. Kalman filtering, ROSE).</rdfs:comment>
    <schema:domainIncludes rdf:resource="http://schema.org/ProcessingInformation"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/processingLevel">
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <schema:domainIncludes rdf:resource="http://schema.org/ProcessingInformation"/>
    <rdfs:label>processingLevel</rdfs:label>
    <rdfs:comment>Processing level applied to the product: "1A", "1B", "1C", "2", "3".</rdfs:comment>
  </rdf:Property>
  <schema:AcquisitionStatusType rdf:about="http://schema.org/ARCHIVED">
    <rdfs:comment>Acquisition status constant.</rdfs:comment>
    <rdfs:label>ARCHIVED</rdfs:label>
  </schema:AcquisitionStatusType>
  <rdf:Property rdf:about="http://schema.org/archivingDate">
    <rdfs:comment>Archiving date time.</rdfs:comment>
    <schema:domainIncludes rdf:resource="http://schema.org/ProductInformation"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/DateTime"/>
    <rdfs:label>archivingDate</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/verticalResolution">
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:comment>lmb: Vertical spacing of data (if regular)
atm: Full width at half maximum of the rows of the vertical averaging kernel matrix
Unit of measure is SI base unit (m) without prefix.</rdfs:comment>
    <rdfs:label>verticalResolution</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/Number"/>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/qualityDegradation">
    <schema:domainIncludes rdf:resource="http://schema.org/QualityInformation"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:label>qualityDegradation</rdfs:label>
    <rdfs:comment>Quality degradation percentage (i.e. uom=’%’).</rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/Number"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/productInformation">
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:domainIncludes rdf:resource="http://schema.org/EarthObservation"/>
    <rdfs:comment>Product information related to the dataset.</rdfs:comment>
    <owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/productInformation"/>
    <schema:isPartOf rdf:resource="http://eo.schema.org"/>
    <rdfs:label>productInformation</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/ProductInformation"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/lastOrbitNumber">
    <schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
    <rdfs:label>lastOrbitNumber</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/Integer"/>
    <rdfs:comment>Acquisition last orbit number.</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/processingMode">
    <schema:domainIncludes rdf:resource="http://schema.org/ProcessingInformation"/>
    <rdfs:comment>Processing mode taken from mission specific code list.
Examples of values are:
•	NRT
•	NOMINAL
•	BACKLOGGED
•	REPROCESSED
•	VALIDATE.</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <rdfs:label>processingMode</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/size">
    <owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/size"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/availabilityTime">
    <rdfs:label>availabilityTime</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/DateTime"/>
    <rdfs:comment>The time when the result becomes available
 dateTime in ISO 8601 format (CCYY-MM-DDThh:mm[:ss[.cc]]Z).</rdfs:comment>
    <schema:domainIncludes rdf:resource="http://schema.org/ProductInformation"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/polarisationChannels">
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
    <rdfs:comment>Polarisation channel transmit/receive configuration.</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/PolarisationChannelsType"/>
    <rdfs:label>polarisationChannels</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/wrsLatitudeGrid">
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:comment>Neutral wrsLatitudeGrid equivalent to frame in track/frame,  J in K/J, etc.</rdfs:comment>
    <rdfs:label>wrsLatitudeGrid</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
  </rdf:Property>
  <schema:SpectralRangeType rdf:about="http://schema.org/INFRARED">
    <rdfs:comment>SpectralRange type constant.</rdfs:comment>
    <rdfs:label>INFRARED</rdfs:label>
  </schema:SpectralRangeType>
  <rdf:Property rdf:about="http://schema.org/ascendingNodeLongitude">
    <rdfs:subPropertyOf rdf:resource="http://schema.org/longitude"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Number"/>
    <rdfs:label>ascendingNodeLongitude</rdfs:label>
    <rdfs:comment>Longitude at ascending node of orbit. Should be expressed in degrees.</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/illuminationZenithAngle">
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionAngles"/>
    <rdfs:label>illuminationZenithAngle</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/Number"/>
    <rdfs:comment>Mean illumination/solar zenith angle in degrees (i.e. uom='deg').</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/platform">
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionInformation"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Platform"/>
    <rdfs:label>platform</rdfs:label>
    <rdfs:comment>The Platform used for the acquisition.  If more than one platform is used for creating the product, then the AcquisitionInformation object occurs more than once.</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
  </rdf:Property>
  <schema:SpectralRangeType rdf:about="http://schema.org/SWIR">
    <rdfs:comment>SpectralRange type constant for Short Wavelength Infrared (SWIR).</rdfs:comment>
    <rdfs:label>SWIR</rdfs:label>
  </schema:SpectralRangeType>
  <rdf:Property rdf:about="http://schema.org/cycleNumber">
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
    <rdfs:label>cycleNumber</rdfs:label>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:comment>Number of Cycles.</rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/Integer"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/acquisitionSubType">
    <rdfs:comment>The broad value defined by the acquisitionType is too restrictive, so mission specific type definition should refer to mission/ground segment dedicated codeSpace.</rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
    <rdfs:label>acquisitionSubType</rdfs:label>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
  </rdf:Property>
  <schema:AntennaLookDirectionType rdf:about="http://schema.org/RIGHT">
    <rdfs:label>RIGHT</rdfs:label>
    <rdfs:comment>Antenna look direction constant.</rdfs:comment>
  </schema:AntennaLookDirectionType>
  <rdf:Property rdf:about="http://schema.org/productContentType">
    <rdfs:label>productContentType</rdfs:label>
    <rdfs:comment>Classification of product according to ground type covered. Note cardinality allows for multiple instances of this property.
Allowed Values:
•	COASTAL
•	CONTINENTAL
•	HYDROLOGY
•	ICE
•	OPEN_OCEAN
•	OTHER
•	REGIONAL.</rdfs:comment>
    <schema:domainIncludes rdf:resource="http://schema.org/ProcessingInformation"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/DefinedTerm"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/tileId">
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <rdfs:comment>While track/frame can be used to represent the first part of an MGRS coordinate (i.g. grid zone), the tileId identifies e.g. the second part of an MGRS coordinate (square identification), e.g. in case of Sentinel.
Used when the world reference system coordinates can not be expressed in X/Y (Track/Frame) terms, such has for UTM tiles. (used for Sentinel-2 L1C granules).</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
    <rdfs:label>tileId</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/instrumentZenithAngle">
    <rdfs:label>instrumentZenithAngle</rdfs:label>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionAngles"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Number"/>
    <rdfs:comment>Mean instrument zenith angle in degrees (i.e. uom='deg').</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
  </rdf:Property>
  <schema:PolarisationChannelsType rdf:about="http://schema.org/HH_VH">
    <rdfs:comment>Polarisation channel constant.</rdfs:comment>
    <rdfs:label>HH_VH</rdfs:label>
  </schema:PolarisationChannelsType>
  <rdf:Property rdf:about="http://schema.org/acquisitionInformation">
    <schema:rangeIncludes rdf:resource="http://schema.org/AcquisitionInformation"/>
    <schema:isPartOf rdf:resource="http://eo.schema.org"/>
    <rdfs:comment>Acquisition information related to the dataset.</rdfs:comment>
    <rdfs:label>acquisitionInformation</rdfs:label>
    <schema:domainIncludes rdf:resource="http://schema.org/EarthObservation"/>
    <owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/acquisitionInformation"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html#44"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/completionTimeFromAscendingNode">
    <rdfs:label>completionTimeFromAscendingNode</rdfs:label>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
    <rdfs:comment>Stop time of acquisition in milliseconds from ascending node date.</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Integer"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/polarisationMode">
    <schema:rangeIncludes rdf:resource="http://schema.org/PolarisationModeType"/>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:label>polarisationMode</rdfs:label>
    <rdfs:comment>Polarisation mode taken from codelist:
S (for single),
D (for dual),
T (for twin),
Q (for quad),
UNDEFINED.</rdfs:comment>
  </rdf:Property>
  <schema:OrbitDirectionType rdf:about="http://schema.org/DESCENDING">
    <rdfs:comment>Orbit direction constant.</rdfs:comment>
    <rdfs:label>DESCENDING</rdfs:label>
  </schema:OrbitDirectionType>
  <rdf:Property rdf:about="http://schema.org/startTimeFromAscendingNode">
    <rdfs:label>startTimeFromAscendingNode</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/Integer"/>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
    <rdfs:comment>Start time of acquisition in milliseconds from ascending node date.</rdfs:comment>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
  </rdf:Property>
  <schema:PolarisationChannelsType rdf:about="http://schema.org/VH_VV">
    <rdfs:label>VH_VV</rdfs:label>
    <rdfs:comment>Polarisation channel constant.</rdfs:comment>
  </schema:PolarisationChannelsType>
  <rdf:Property rdf:about="http://schema.org/compositeType">
    <schema:domainIncludes rdf:resource="http://schema.org/ProcessingInformation"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Duration"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:seeAlso rdf:resource="https://en.wikipedia.org/wiki/ISO_8601#Durations"/>
    <rdfs:comment>Type of composite product expressed as timeperiod that the composite product covers using ISO 8601 duration format P[n]Y[n]M[n]D encoding , e.g. P10D for a 10 day composite.</rdfs:comment>
    <rdfs:label>compositeType</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/samplingRate">
    <schema:rangeIncludes rdf:resource="http://schema.org/Number"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:comment>Rate at which samples are provided in product. Some products may contain more than one sampling rate, e.g. 1kHz and 20kHz. Cardinality is therefore zero to many.  Unit of measure (Hz) is SI derived unit without prefix.</rdfs:comment>
    <rdfs:label>samplingRates</rdfs:label>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/wavelengths">
    <rdfs:label>wavelengths</rdfs:label>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:comment>List of discrete wavelengths observed in the product.</rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/WavelengthInformation"/>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
  </rdf:Property>
  <schema:SpectralRangeType rdf:about="http://schema.org/VISIBLE">
    <rdfs:label>VISIBLE</rdfs:label>
    <rdfs:comment>SpectralRange type constant.</rdfs:comment>
  </schema:SpectralRangeType>
  <rdf:Property rdf:about="http://schema.org/pitch">
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:comment>Satellite pitch angle in degrees (i.e. uom='deg').</rdfs:comment>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionAngles"/>
    <rdfs:label>pitch</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/Number"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/status">
    <schema:domainIncludes rdf:resource="http://schema.org/EarthObservation"/>
    <rdfs:label>status</rdfs:label>
    <schema:isPartOf rdf:resource="http://eo.schema.org"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/AcquisitionStatusType"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/status"/>
    <rdfs:comment>Refers to product status.

Fixed Values :
 - ARCHIVED
 - ACQUIRED
 - CANCELLED
 - FAILED
 - PLANNED
 - POTENTIAL
 - REJECTED
 - QUALITYDEGRADED.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/platformShortName">
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/platformShortName"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <schema:domainIncludes rdf:resource="http://schema.org/Platform"/>
    <rdfs:comment>Platform short name (e.g. "Seasat" or "ENVISAT").</rdfs:comment>
    <rdfs:label>platformShortName</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/acquisitionType">
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <rdfs:label>acquisitionType</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/AcquisitionType"/>
    <rdfs:comment>AcquisitionType can be one of: 
 - NOMINAL
 - CALIBRATION
 - OTHER.</rdfs:comment>
    <schema:domainIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
  </rdf:Property>
  <schema:SpectralRangeType rdf:about="http://schema.org/LWIR">
    <rdfs:label>LWIR</rdfs:label>
    <rdfs:comment>SpectralRange type constant for Long Wavelength Infrared (LWIR).</rdfs:comment>
  </schema:SpectralRangeType>
  <rdf:Property rdf:about="http://schema.org/ascendingNodeDate">
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/DateTime"/>
    <rdfs:label>ascendingNodeDate</rdfs:label>
    <schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
    <rdfs:comment>UTC date and time at ascending node of orbit.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/orbitNumber">
    <schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
    <dcterms:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
