<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dct="http://purl.org/dc/terms/" xmlns:vann="http://purl.org/vocab/vann/" xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:schema="http://schema.org/">
	<owl:Ontology rdf:about="http://eo.schema.org/">
		<dct:title>EOVOC - Earth Observation extension of Schema.org</dct:title>
		<owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.1.0</owl:versionInfo>
		<dct:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2020-12-11</dct:date>
		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This vocabulary defines the classes and properties required to be added to Schema.org to have similar expressiveness as OGC 17-003r2.</rdfs:comment>
		<vann:preferredNamespacePrefix>eoschema</vann:preferredNamespacePrefix>
		<vann:preferredNamespaceUri>http://eo.schema.org/</vann:preferredNamespaceUri>
		<dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Yves Coene, Spacebel s.a.</dct:creator>
	</owl:Ontology>
	<!-- 
       Table numbers refer to the tables in the OGC specification  
	   https://docs.ogc.org/is/17-003r2/17-003r2.html
-->
	<!-- 
       Table 4: EarthObservation Class and properties 
-->
	<rdfs:Class rdf:about="http://schema.org/Dataset">
		<owl:equivalentClass rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
		<owl:equivalentClass rdf:resource="http://rdfs.org/ns/void#Dataset"/>
		<rdfs:label>Dataset</rdfs:label>
		<rdfs:subClassOf rdf:resource="http://schema.org/CreativeWork"/>
		<owl:equivalentClass rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
		<rdfs:comment>A body of structured information describing some topic(s) of interest.</rdfs:comment>
		<dct:source rdf:resource="http://www.w3.org/wiki/WebSchemas/SchemaDotOrgSources#source_DatasetClass"/>
	</rdfs:Class>
	<!-- 
       Table 5: Properties Class and properties 
   -->
	<schema:AcquisitionStatusType rdf:about="http://schema.org/ARCHIVED">
		<rdfs:comment>Acquisition status constant.</rdfs:comment>
		<rdfs:label>ARCHIVED</rdfs:label>
	</schema:AcquisitionStatusType>
	<schema:AcquisitionStatusType rdf:about="http://schema.org/ACQUIRED">
		<rdfs:comment>Acquisition status constant.</rdfs:comment>
		<rdfs:label>ACQUIRED</rdfs:label>
	</schema:AcquisitionStatusType>
	<schema:AcquisitionStatusType rdf:about="http://schema.org/CANCELLED">
		<rdfs:comment>Acquisition status constant.</rdfs:comment>
		<rdfs:label>CANCELLED</rdfs:label>
	</schema:AcquisitionStatusType>
	<schema:AcquisitionStatusType rdf:about="http://schema.org/FAILED">
		<rdfs:comment>Acquisition status constant.</rdfs:comment>
		<rdfs:label>FAILED</rdfs:label>
	</schema:AcquisitionStatusType>
	<schema:AcquisitionStatusType rdf:about="http://schema.org/ACQUIRED">
		<rdfs:comment>Acquisition status constant.</rdfs:comment>
		<rdfs:label>ACQUIRED</rdfs:label>
	</schema:AcquisitionStatusType>
	<schema:AcquisitionStatusType rdf:about="http://schema.org/PLANNED">
		<rdfs:comment>Acquisition status constant.</rdfs:comment>
		<rdfs:label>PLANNED</rdfs:label>
	</schema:AcquisitionStatusType>
	<schema:AcquisitionStatusType rdf:about="http://schema.org/POTENTIAL">
		<rdfs:comment>Acquisition status constant.</rdfs:comment>
		<rdfs:label>POTENTIAL</rdfs:label>
	</schema:AcquisitionStatusType>
	<schema:AcquisitionStatusType rdf:about="http://schema.org/REJECTED">
		<rdfs:comment>Acquisition status constant.</rdfs:comment>
		<rdfs:label>REJECTED</rdfs:label>
	</schema:AcquisitionStatusType>
	<schema:AcquisitionStatusType rdf:about="http://schema.org/QUALITYDEGRADED">
		<rdfs:comment>Acquisition status constant.</rdfs:comment>
		<rdfs:label>QUALITYDEGRADED</rdfs:label>
	</schema:AcquisitionStatusType>
	<rdfs:Class rdf:about="http://schema.org/AcquisitionStatusType">
		<rdfs:comment>Enumeration of acquisition status constants.</rdfs:comment>
		<schema:isPartOf rdf:resource="http://eo.schema.org"/>
		<rdfs:subClassOf rdf:resource="http://schema.org/StatusEnumeration"/>
		<rdfs:label>AcquisitionStatusType</rdfs:label>
		<owl:equivalentClass rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/statusType"/>
	</rdfs:Class>
	<rdf:Property rdf:about="http://schema.org/status">
		<schema:isPartOf rdf:resource="http://eo.schema.org"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/AcquisitionStatusType"/>
		<rdfs:comment>Refers to product status.

Fixed Values :
 - ARCHIVED
 - ACQUIRED
 - CANCELLED
 - FAILED
 - PLANNED
 - POTENTIAL
 - REJECTED
 - QUALITYDEGRADED.</rdfs:comment>
		<rdfs:label>status</rdfs:label>
		<schema:domainIncludes rdf:resource="http://schema.org/Dataset"/>
		<owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/status"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/acquisitionInformation">
		<schema:isPartOf rdf:resource="http://eo.schema.org"/>
		<rdfs:label>acquisitionInformation</rdfs:label>
		<schema:rangeIncludes rdf:resource="http://schema.org/AcquisitionInformation"/>
		<schema:domainIncludes rdf:resource="http://schema.org/Dataset"/>
		<rdfs:comment>Acquisition information related to the dataset.</rdfs:comment>
		<owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/acquisitionInformation"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html#44"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/productInformation">
		<schema:isPartOf rdf:resource="http://eo.schema.org"/>
		<rdfs:label>productInformation</rdfs:label>
		<schema:rangeIncludes rdf:resource="http://schema.org/ProductInformation"/>
		<schema:domainIncludes rdf:resource="http://schema.org/Dataset"/>
		<rdfs:comment>Product information related to the dataset.</rdfs:comment>
		<owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/productInformation"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	</rdf:Property>
	<!-- 
       Table 9: DataIdentification Class and properties 
    -->
	<rdf:Property rdf:about="http://schema.org/parentIdentifier">
		<schema:isPartOf rdf:resource="http://eo.schema.org"/>
		<rdfs:label>parentIdentifier</rdfs:label>
		<rdfs:comment>Collection identifier.</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/Dataset"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
	</rdf:Property>
	<!-- 
       Table 11: VerticalSpatialDomain Class and properties 
-->
	<rdfs:Class rdf:about="http://schema.org/VerticalSpatialDomain">
		<schema:isPartOf rdf:resource="http://eo.schema.org"/>
		<rdfs:label>VerticalSpatialDomain</rdfs:label>
		<rdfs:comment>Contains the properties related to the spatial extent in the vertical dimension.  Its properties are inherited by the AcquisitionParameters object. 
  UnitText used to express minValue (lowestLocation) and maxValue (highestLocation) should be meter (m) or bar (bar). Default is meter (m).</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://schema.org/QuantitativeValue"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<owl:equivalentClass rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/VerticalSpatialDomain"/>
	</rdfs:Class>
	<!-- 
       Table 12: OrbitParameters Class and properties 
-->
	<rdfs:Class rdf:about="http://schema.org/OrbitParameters">
		<schema:isPartOf rdf:resource="http://eo.schema.org"/>
		<rdfs:label>OrbitParameters</rdfs:label>
		<rdfs:comment>Contains the properties related to the orbit.  Its properties are inherited by the AcquisitionParameters object.</rdfs:comment>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdfs:subClassOf rdf:resource="http://schema.org/Intangible"/>
		<owl:equivalentClass rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/OrbitParameters"/>
	</rdfs:Class>
	<schema:OrbitDirectionType rdf:about="http://schema.org/DESCENDING">
		<rdfs:comment>Orbit direction constant.</rdfs:comment>
		<rdfs:label>DESCENDING</rdfs:label>
	</schema:OrbitDirectionType>
	<schema:OrbitDirectionType rdf:about="http://schema.org/ASCENDING">
		<rdfs:comment>Orbit direction constant.</rdfs:comment>
		<rdfs:label>ASCENDING</rdfs:label>
	</schema:OrbitDirectionType>
	<rdfs:Class rdf:about="http://schema.org/OrbitDirectionType">
		<schema:isPartOf rdf:resource="http://eo.schema.org"/>
		<rdfs:comment>Enumeration of orbit direction constants.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://schema.org/Enumeration"/>
		<rdfs:label>OrbitDirectionType</rdfs:label>
		<owl:equivalentClass rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/OrbitDirectionType"/>
	</rdfs:Class>
	<rdf:Property rdf:about="http://schema.org/orbitDirection">
		<schema:isPartOf rdf:resource="http://eo.schema.org"/>
		<rdfs:label>orbitDirection</rdfs:label>
		<rdfs:comment>Acquisition orbit direction at the start of the acquisition/product.
Values:
•	ASCENDING
•	DESCENDING</rdfs:comment>
		<rdfs:comment>Product information related to the dataset.</rdfs:comment>
		<owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/orbitDirection"/>
		<schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/OrbitDirectionType"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/lastOrbitDirection">
		<rdfs:label>lastOrbitDirection</rdfs:label>
		<rdfs:comment>Acquisition orbit direction at the end of the acquisition/product. Assumed to be identical to orbitDirection if not present.
Values:
•	ASCENDING
•	DESCENDING</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/OrbitDirectionType"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/orbitDuration">
		<rdfs:label>orbitDuration</rdfs:label>
		<rdfs:comment>Actual orbit duration in ISO 8601 duration format (range is TBD).</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/Duration"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
		<rdfs:subPropertyOf rdf:about="http://schema.org/duration"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/orbitNumber">
		<rdfs:label>orbitNumber</rdfs:label>
		<rdfs:comment>Acquisition orbit number.</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/Integer"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/lastOrbitNumber">
		<rdfs:label>lastOrbitNumber</rdfs:label>
		<rdfs:comment>Acquisition last orbit number.</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/Integer"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/ascendingNodeDate">
		<rdfs:label>ascendingNodeDate</rdfs:label>
		<rdfs:comment>UTC date and time at ascending node of orbit.</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/DateTime"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/ascendingNodeLongitude">
		<rdfs:label>ascendingNodeLongitude</rdfs:label>
		<rdfs:comment>Longitude at ascending node of orbit. Should be expressed in degrees.</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/OrbitParameters"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/Number"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
		<rdfs:subPropertyOf rdf:about="http://schema.org/longitude"/>
	</rdf:Property>
	<!-- 
       Table 14: AcquisitionInformation Class and properties 
-->
	<rdfs:Class rdf:about="http://schema.org/AcquisitionInformation">
		<rdfs:label>AcquisitionInformation</rdfs:label>
		<rdfs:comment>Acquisition information of the Earth Observation as defined in OGC 17-003r2.</rdfs:comment>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html#44"/>
		<rdfs:subClassOf rdf:resource="http://schema.org/StructuredValue"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<owl:equivalentClass rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/AcquisitionInformation"/>
	</rdfs:Class>
	<rdf:Property rdf:about="http://schema.org/platform">
		<rdfs:label>platform</rdfs:label>
		<schema:domainIncludes rdf:resource="http://schema.org/AcquisitionInformation"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/Platform"/>
		<rdfs:comment>The Platform used for the acquisition.  If more than one platform is used for creating the product, then the AcquisitionInformation object occurs more than once.</rdfs:comment>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/instrument">
		<rdfs:label>instrument</rdfs:label>
		<schema:domainIncludes rdf:resource="http://schema.org/AcquisitionInformation"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/Instrument"/>
		<rdfs:comment>The Instrument/Sensor used for the acquisition.
If more than one instrument is used for creating the product, then the AcquisitionInformation object occurs more than once.</rdfs:comment>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/acquisitionParameters">
		<rdfs:label>acquisitionParameters</rdfs:label>
		<schema:domainIncludes rdf:resource="http://schema.org/AcquisitionInformation"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/AcquisitionParameters"/>
		<rdfs:comment>The acquisition parameters (i.e. pointing angles, etc.).</rdfs:comment>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	</rdf:Property>
	<!-- 
       Table 15: Platform Class and properties 
-->
	<rdf:Class rdf:about="http://schema.org/Platform">
		<rdfs:label>Platform</rdfs:label>
		<rdfs:comment>Contains the properties of the platform (satellite) that was used to perform the observation.  Use as "id" the URI defined by GCMD to identify the platform.  A list of URI can be downloaded from https://gcmdservices.gsfc.nasa.gov/static/kms/platforms/platforms.rdf.</rdfs:comment>
		<rdfs:seeAlso rdf:resource="https://gcmdservices.gsfc.nasa.gov/static/kms/platforms/platforms.rdf"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdfs:subClassOf rdf:resource="http://schema.org/StructuredValue"/>
	</rdf:Class>
	<rdf:Property rdf:about="http://schema.org/platformShortName">
		<rdfs:label>platformShortName</rdfs:label>
		<rdfs:comment>Platform short name (e.g. "Seasat" or "ENVISAT").</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/Platform"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/platformShortName"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://schema.org/platformSerialIdentifier">
		<rdfs:label>platformSerialIdentifier</rdfs:label>
		<rdfs:comment>Platform serial identifier (e.g. for Seasat : 1).</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/Platform"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/platformSerialIdentifier"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
	</rdf:Property>
	<rdf:Class rdf:about="http://schema.org/OrbitType">
		<rdfs:label>OrbitType</rdfs:label>
		<rdfs:comment>Enumeration of orbitType constants.</rdfs:comment>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdfs:subClassOf rdf:resource="http://schema.org/Enumeration"/>
	</rdf:Class>
	<schema:OrbitType rdf:about="http://schema.org/GEO">
		<rdfs:label>GEO</rdfs:label>
		<rdfs:comment>Orbit type constant.</rdfs:comment>
	</schema:OrbitType>
	<schema:OrbitType rdf:about="http://schema.org/LEO">
		<rdfs:label>LEO</rdfs:label>
		<rdfs:comment>Orbit type constant.</rdfs:comment>
	</schema:OrbitType>
	<rdf:Property rdf:about="http://schema.org/orbitType">
		<rdfs:label>orbitType</rdfs:label>
		<rdfs:comment>High level characterisation of main mission types taken from a codelist.</rdfs:comment>
		<schema:domainIncludes rdf:resource="http://schema.org/Platform"/>
		<schema:rangeIncludes rdf:resource="http://schema.org/OrbitType"/>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<owl:equivalentProperty rdf:resource="http://www.opengis.net/ont/eo-geojson/1.0/orbitType"/>
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	</rdf:Property>
	<!-- 
       Table 16: Instrument Class and properties 
-->
	<rdf:Class rdf:about="http://schema.org/Instrument">
		<rdfs:label>Instrument</rdfs:label>
		<rdfs:comment>Contains the properties of the instrument that was used to perform the observation.  Use as "id" the URI defined by GCMD to identify the instrument.  A list of URI can be downloaded from https://gcmdservices.gsfc.nasa.gov/static/kms/instruments/instruments.rdf.</rdfs:comment>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdfs:subClassOf rdf:resource="http://schema.org/StructuredValue"/>
	</rdf:Class>
	<rdf:Class rdf:about="http://schema.org/SensorType">
		<rdfs:label>SensorType</rdfs:label>
		<rdfs:comment>Enumeration of sensorType constants.</rdfs:comment>
		<dct:source rdf:resource="http://docs.opengeospatial.org/is/17-003r2/17-003r2.html"/>
		<rdfs:subClassOf rdf:resource="http://schema.org/Enumeration"/>
	</rdf:Class>
	<schema:SensorType rdf:about="http://schema.org/OPTICAL">
		<rdfs:label>OPTICAL</rdfs:label>
		<rdfs:comment>Sensor type constant.</rdfs:comment>
	</schema:SensorType>
	<schema:SensorType rdf:about="http://schema.org/RADAR">
		<rdfs:label>RADAR</rdfs:label>
		<rdfs:comment>Sensor type constant.</rdfs:comment>
	</schema:SensorType>
	<schema:SensorType rdf:about="http://schema.org/ALTIMETRIC">
		<rdfs:label>ALTIMETRIC</rdfs:label>
		<rdfs:comment>Sensor type constant.</rdfs:comment>
	</schema:SensorType>
	<schema:SensorType rdf:about="http://schema.org/ATMOSPHERIC">
		<rdfs:label>ATMOSPHERIC</rdfs:label>
		<rdfs:comment>Sensor type constant.</rdfs:comment>
	</schema:SensorType>
	<schema:SensorType rdf:about="http://schema.org/LIMB">
		<rdfs:label>LIMB</rdfs:label>
		<rdfs:comment>Sensor type constant.</rdfs:comment>
	</schema:SensorType>
	<rdf:Property rdf:about="http://schema.org/sensorType">
